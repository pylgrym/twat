<% provide(:title, "Sign in") %>   
<h1>Sign in </h1>

<!-- p334 added -->
<div class="row">
  <div class="span6 offset3 mydialog">
  	<%= form_for(:session, url: sessions_path, html: { class: "myform"} ) do |f| %>

  	<%= f.label :email %>
  	<%= f.text_field :email, autofocus: true, placeholder: "name@youremail.com", class: "form-control" %>

  	<%= f.label :password %>
  	<%= f.password_field :password, class: "form-control" %>
    <br>

  	<%= f.submit "Sign in (p334)", class: "btn btn-large btn-primary" %>
  	<% end %>

  	<p>New user? <%= link_to "Sign up now!", signup_path %> </p>

  </div>
</div>
<!-- end block p334 -->
